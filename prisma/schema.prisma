generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Archive {
id                    String      @id     @default(uuid())
archiveName           String
successUnzipping      Boolean?
file                 FileHtml?
}

model FileHtml {
id                    String      @id     @default(uuid())
archive               Archive     @relation(fields: [archiveId], references: [id])
archiveId             String      @unique
filename              String
successConversion     Boolean? 
}

